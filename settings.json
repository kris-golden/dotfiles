{
    "firefox.port": 3000,
    "debug.javascript.autoAttachFilter": "smart",
    "window.zoomLevel": 1,
    "breadcrumbs.icons": false,
    "livePreview.portNumber": 4000,
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.compactFolders": false,
    "remote.SSH.remotePlatform": {
        "aws-forge": "linux",
        "forge": "linux",
        "bastionk": "linux"
    },
    

    "[csv]": {},
    "[dynamic csv]": {},
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },


    "dart.lineLength": 180,
    "dart.closingLabels": false,
    "dart.enableSdkFormatter": false,
    "dart.customDevTools": {},
    "dart.debugExternalPackageLibraries": true,
    "dart.debugSdkLibraries": true,
    "[dart]": {
        "editor.tabSize": 3,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            180
        ],
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },


    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.semanticTokens": true
    },


    "git.openRepositoryInParentFolders": "never",
    "git.enableSmartCommit": true,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,

    
    "editor.parameterHints.cycle": false,
    "editor.suggest.showTypeParameters": false,
    "editor.parameterHints.enabled": false,
    "editor.inlineSuggest.showToolbar": "always",
    "editor.minimap.enabled": false,
    "editor.tabSize": 3,
    "editor.indentSize": "tabSize",
    "editor.lineHeight": 1.3,
    "editor.lightbulb.enabled": "off",
    "editor.fontWeight": "normal",
    "editor.fontSize": 15,
    "editor.fontVariations": false,
    "editor.fontFamily": "FiraCode Nerd Font Mono",
    "editor.rulers": [],
    "editor.acceptSuggestionOnEnter": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "other": "inline"
    },
    "editor.tokenColorCustomizations": {
        "[Field Lights]": {
            "variables": "#DDDDDD",
            "strings": "#338833",
            "types": "#77BBEE",
            "keywords": "#CC6000",  
        },
        "[Default Light+]" :{
            
            //"variables": "#0532d6",
            //"strings": "#45dced",
            "strings": "#dd4400",
            //"keywords": "#0000FF",
            //"types": "#f10707",
            "functions": "#4a8ed1",
            "comments": "#049a04",
            //"numbers": "#FF22ff",
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.function",
                    ],
                    "settings": {
                        "fontStyle": "italic bold",
                    }
                },
                {
                    "scope": [
                        //"string.quoted",
                        //"variable"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                    }

                }
            ]

        }
    },


    "workbench.tree.indent": 16,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Default Light+",
    "workbench.colorCustomizations": {
        "[Field Lights]": {
            "editorBracketHighlight.foreground1": "#DDDDDD",
            "editorBracketHighlight.foreground3": "#0088FF",
            "editorBracketHighlight.foreground2": "#33AAAA",
            "editorBracketHighlight.foreground4": "#DD8800",
            "editor.background": "#121212",
            "editor.foreground":"default",
            "sideBar.background": "#111119",
            "panel.background": "#181820"
        },
        "[Default Light+]" : {
            //"editor.selectionBackground": "#FFBBBB",
            "editor.findMatchBackground": "#AAAAFF",
            "editor.wordHighlightTextBackground": "#00555508",
            //"editor.background": "#FeFefe",

        }
    },


    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
    "terminal.integrated.customGlyphs": false,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.cursorStyle": "underline",

        
}